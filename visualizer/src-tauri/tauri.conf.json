{
	"build": {
		"beforeDevCommand": "",
		"beforeBuildCommand": "",
		"devPath": "../src",
		"distDir": "../src",
		"withGlobalTauri": true
	},
	"package": {
		"productName": "nlt-panel-visualizer",
		"version": "0.0.0"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"shell": {
				"all": false,
				"open": true
			},
			"fs": {
				"scope": ["$RESOURCE/**"],
				"readFile": true
			},
			"path": {
				"all": true
			}
		},
		"bundle": {
			"active": true,
			"targets": "all",
			"identifier": "com.tauri.dev",
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"resources": ["static/*"]
		},
		"security": {
			"csp": null
		},
		"windows": [
			{
				"url": "index.html",
				"title": "nlt-panel-visualizer",
				"label": "main",
				"width": 800,
				"height": 600,
				"fullscreen": false,
				"resizable": true,
				"transparent": true,
				"center": true,
				"decorations": false,
				"visible": true
			}
		]
	}
}
